    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed": "ts-node src/db/seed.ts",
    "seed:employees": "ts-node src/db/seed-employees.ts",
    "seed:employees:all": "ts-node src/db/seed-employees-all.ts",
    "db:export": "ts-node src/db/export-all-data.ts",
    "db:import": "ts-node src/db/import-data.ts",
    "db:import:direct": "ts-node src/db/import-direct.ts",
    "db:import:supabase": "ts-node src/db/import-to-supabase.ts",
    "db:import:sql": "ts-node src/db/import-sql-to-supabase.ts",
    "db:setup:supabase": "ts-node src/db/setup-supabase.ts",
    "db:exports:list": "ts-node src/db/manage-exports.ts list",
    "db:anonymize": "ts-node src/db/anonymize-data.ts",
    "db:cleanup:duplicates": "ts-node src/db/remove-all-duplicates.ts",
    "db:remove:duplicates": "ts-node src/db/remove-duplicates-advanced.ts",
    "db:fix:client-ids": "ts-node src/db/fix-client-ids.ts",
    "db:restore:clients": "ts-node src/db/restore-clients-from-backup.ts",
    "db:check:null-ids": "ts-node src/db/check-null-ids.ts",
    "db:fix:all-null-ids": "ts-node src/db/fix-all-null-ids.ts",
    "db:setup:auto-increment": "ts-node src/db/setup-auto-increment.ts"
